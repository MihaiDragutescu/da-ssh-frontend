{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@App/*": [
        "./*"
      ],
      "@Components-ui/*": [
        "./components/ui/*"
      ],
      "@Fonts/*": [
        "./assets/fonts/*"
      ],
      "@Home/*": [
        "./pages/Home/*"
      ],
      "@Images/*": [
        "./assets/images/*"
      ],
      "@Styles/*": [
        "./styles/*"
    ],
  },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
